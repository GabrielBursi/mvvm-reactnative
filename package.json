{
	"name": "mvvm-react",
	"version": "1.0.0",
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "react-native start -- --reset-cache",
		"dev": "npm run android --active-arch-only --clear-cache",
		"doctor": "react-native doctor",
		"check-dependencies": "rnx-align-deps",
		"fix-dependencies": "rnx-align-deps --write",
		"assets": "npx react-native-asset",
		"test": "jest --passWithNoTests --maxWorkers=50% --verbose",
		"test:watch": "jest --watchAll --maxWorkers=25% --verbose",
		"e2e:build": "detox build --configuration android.emu.debug",
		"e2e:test": "detox test --configuration android.emu.debug -l warn --take-screenshots failing",
		"generate:c": "plop --plopfile ./generators/component.mjs && npm run format",
		"generate:s": "plop --plopfile ./generators/screen.mjs && npm run format",
		"generate:h": "plop --plopfile ./generators/hook.mjs && npm run format",
		"generate:t": "plop --plopfile ./generators/template.mjs && npm run format",
		"generate:e2e": "plop --plopfile ./generators/e2e.mjs && npm run format",
		"rdt": "react-devtools",
		"lint": "eslint --ext .tsx --ext .ts src/",
		"lint:fix": "npm run lint -- --fix",
		"prettier:write": "npx prettier --write **/*.{ts,tsx,json}",
		"prettier:format": "prettier --check ./src",
		"format": "npm run prettier:write && npm run lint:fix",
		"husky": "husky",
		"storybook-generate": "sb-rn-get-stories"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^1.22.0",
		"@react-navigation/bottom-tabs": "^6.5.11",
		"@react-navigation/drawer": "^6.6.6",
		"@react-navigation/native": "^6.0.8",
		"@react-navigation/native-stack": "^6.9.17",
		"@react-navigation/stack": "^6.3.21",
		"@tanstack/react-query": "^5.17.10",
		"@tanstack/react-query-devtools": "^5.17.10",
		"axios": "^1.7.4",
		"polished": "^4.3.1",
		"react": "18.2.0",
		"react-dom": "^18.2.0",
		"react-native": "^0.73.0",
		"react-native-config": "^1.5.1",
		"react-native-gesture-handler": "^2.14.0",
		"react-native-mmkv": "^2.11.0",
		"react-native-normalize": "^1.0.1",
		"react-native-reanimated": "^3.6.0",
		"react-native-responsive-fontsize": "^0.5.1",
		"react-native-safe-area-context": "^4.8.2",
		"react-native-screens": "^3.28.0",
		"styled-components": "^6.1.8"
	},
	"devDependencies": {
		"@babel/core": "^7.23.7",
		"@babel/plugin-proposal-private-methods": "^7.18.6",
		"@babel/plugin-transform-class-properties": "^7.23.3",
		"@babel/plugin-transform-private-property-in-object": "^7.23.4",
		"@babel/preset-env": "^7.23.9",
		"@babel/preset-flow": "^7.23.3",
		"@babel/preset-react": "^7.23.3",
		"@babel/preset-typescript": "^7.23.3",
		"@babel/runtime": "7.21.0",
		"@commitlint/cli": "^19.2.1",
		"@commitlint/config-conventional": "^19.1.0",
		"@gorhom/bottom-sheet": "^5.0.5",
		"@react-native-community/slider": "^4.5.5",
		"@react-native/babel-preset": "^0.73.0",
		"@react-native/eslint-config": "^0.73.2",
		"@react-native/metro-config": "^0.73.0",
		"@react-native/typescript-config": "0.73.1",
		"@rnx-kit/align-deps": "^2.5.0",
		"@storybook/addon-ondevice-actions": "^8.3.10",
		"@storybook/addon-ondevice-backgrounds": "^8.3.10",
		"@storybook/addon-ondevice-controls": "^8.3.10",
		"@storybook/addon-ondevice-notes": "^8.3.10",
		"@storybook/react": "^8.3.6",
		"@storybook/react-native": "^8.3.10",
		"@tanstack/eslint-plugin-query": "^5.50.1",
		"@testing-library/jest-native": "^5.4.3",
		"@testing-library/react-native": "^12.4.3",
		"@tsconfig/react-native": "^3.0.2",
		"@types/babel__core": "~7.20.5",
		"@types/babel__preset-env": "~7.9.6",
		"@types/eslint": "~8.56.5",
		"@types/eslint-config-prettier": "~6.11.3",
		"@types/fast-text-encoding": "~1.0.3",
		"@types/jest": "^29.5.12",
		"@types/lint-staged": "~13.3.0",
		"@types/metro": "~0.76.3",
		"@types/metro-config": "~0.76.3",
		"@types/metro-resolver": "~0.76.3",
		"@types/node": "^20.11.24",
		"@types/react": "^18.2.6",
		"@types/react-devtools": "~3.6.2",
		"@types/react-dom": "~18.2.19",
		"@types/react-native-asset": "^2.0.2",
		"@types/react-test-renderer": "^18.0.0",
		"@types/styled-components-react-native": "^5.2.5",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"babel-jest": "^29.7.0",
		"babel-loader": "^8.3.0",
		"babel-plugin-module-resolver": "^5.0.0",
		"detox": "^20.18.3",
		"eslint": "^8.57.0",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-better-styled-components": "^1.1.2",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jest": "^28.5.0",
		"eslint-plugin-jest-formatting": "^3.1.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-json-format": "^2.0.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-native": "^4.1.0",
		"eslint-plugin-react-redux": "^4.1.0",
		"eslint-plugin-sonarjs": "^2.0.4",
		"eslint-plugin-storybook": "^0.10.1",
		"eslint-plugin-styled-components-a11y": "^2.1.32",
		"eslint-plugin-testing-library": "^6.2.2",
		"fast-text-encoding": "^1.0.6",
		"husky": "^9.1.7",
		"jest": "^29.2.1",
		"lint-staged": "^15.2.2",
		"metro": "^0.80.0",
		"metro-babel-register": "0.74.1",
		"metro-config": "^0.80.0",
		"metro-react-native-babel-transformer": "0.74.1",
		"metro-resolver": "^0.80.0",
		"msw": "^2.2.2",
		"plop": "^4.0.1",
		"prettier": "^3.2.5",
		"pretty-quick": "^4.0.0",
		"react-devtools": "^5.0.0",
		"react-native-asset": "^2.1.1",
		"react-native-flipper": "^0.212.0",
		"react-native-svg": "^15.8.0",
		"react-native-url-polyfill": "^2.0.0",
		"react-test-renderer": "18.2.0",
		"reactotron-react-native": "^5.1.0",
		"reactotron-react-native-mmkv": "^0.2.3",
		"reactotron-redux": "^3.1.7",
		"ts-jest": "^29.1.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "5.0.4",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	},
	"engines": {
		"node": ">=18"
	},
	"rnx-kit": {
		"kitType": "app",
		"alignDeps": {
			"requirements": [
				"react-native@0.73"
			],
			"capabilities": [
				"animation",
				"babel-preset-react-native",
				"core",
				"core-android",
				"core-ios",
				"core/metro-config",
				"datetime-picker",
				"gestures",
				"hooks",
				"jest",
				"metro",
				"metro-config",
				"metro-resolver",
				"navigation/native",
				"navigation/stack",
				"react",
				"react-dom",
				"react-test-renderer",
				"safe-area",
				"screens",
				"storage"
			]
		},
		"bundle": {
			"treeShake": true,
			"plugins": [
				"@rnx-kit/metro-plugin-cyclic-dependencies-detector",
				"@rnx-kit/metro-plugin-duplicates-checker",
				[
					"@rnx-kit/metro-plugin-typescript",
					{
						"throwOnError": true
					}
				]
			]
		}
	}
}
